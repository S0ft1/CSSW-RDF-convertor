{
  "name": "@cssw-rdf-convertor/core",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/core/src",
    "tags": [
      "env:library"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/core/dist",
          "main": "packages/core/src/index.ts",
          "tsConfig": "packages/core/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "jsonld": "^8.3.3",
    "mnemonist": "^0.40.2",
    "n3": "^1.23.1",
    "quadstore": "^15.0.0"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15",
    "@types/n3": "^1.21.1",
    "@types/node": "^22.13.5"
  }
}
